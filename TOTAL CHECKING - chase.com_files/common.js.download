	/*Search panel*/
	$("#main").on("click",".primary-search-icon",function(e){
			e.preventDefault();
			e.stopPropagation();
			$(this).parent().addClass("dis-none");
			$(this).parent().parent().addClass("pre-active active");
			
			$(".conversation-deck-primary-search").removeClass("dis-none").css("width","100%");
			$(".conversation-deck-search-box").addClass("is-active").css("opacity","1");
			
			$(".search-suggestions").removeClass("dis-none");
			$("#primary-search-input").css({"width":"100%","opacity":"1"});
			$(".conversation-deck-search-box").focus();
		});
		
		$(document).click(function(e){
			
			var container = $("#conversation-deck-search");
			if (!container.is(e.target)
			&& (container.has(e.target).length === 0))                       
			{
				$(".primary-search-icon").parent().removeClass("dis-none");
				$(".primary-search-icon").parent().parent().removeClass("pre-active active");
				
				$(".conversation-deck-primary-search").addClass("dis-none").css("width","");
				$(".conversation-deck-search-box").removeClass("is-active").css("opacity","0");
			
				$(".search-suggestions").addClass("dis-none");
				
			}
		});
	
	$("#main").on("keyup",".conversation-deck-search-box", function() {
			
			var textval = $(this).val();
			var regexp = /routi/g;
			
			var found = textval.match(regexp);
			
			
			
			if(textval != ""){
				$(".default-search-options").addClass("dis-none");
				$(".primary-search-clear-text").removeClass("dis-none");
				$(".search-suggestions-advisory").removeClass("dis-none");
				
				if (found){
					$("#primary-search-options-list").removeClass("dis-none");
					$("#primary-search-default-options,.default-search-options").addClass("dis-none");
					$(".search-suggestions-advisory").addClass("dis-none");
					$(".inner-container").css("padding-bottom","10px");
				}
			}
			else if(textval == ""){
				$(".default-search-options").removeClass("dis-none");
				$(".primary-search-clear-text, #primary-search-options-list").addClass("dis-none");
				$(".search-suggestions-advisory").addClass("dis-none");
			}
			
		});
		$("#main").on("click",".primary-search-clear-text",function() {
			$(".default-search-options").removeClass("dis-none");
			$(".primary-search-clear-text").addClass("dis-none");
			$(".search-suggestions-advisory").addClass("dis-none");
			$("#primary-search-options-list").addClass("dis-none");
			
		});
		
		$("#main").on("click",".primary-search-option",function() {
			$(".primary-search-icon").parent().removeClass("dis-none");
			$(".primary-search-icon").parent().parent().removeClass("pre-active active");
			
			$(".conversation-deck-primary-search").addClass("dis-none").css("width","");
			$(".conversation-deck-search-box").removeClass("is-active").css("opacity","0");
		
			$(".search-suggestions").addClass("dis-none");
			
			$("#summaryContainer").hide();
			$('#search-deck').hide();
			$('#primary-search-result').show();
			
			setBodyHgt();
		
		});
		
		$("#main").on("click","#closeSearchResultX",function() {
			$('#primary-search-result').hide();
			
			setBodyHgt();
		});
	
