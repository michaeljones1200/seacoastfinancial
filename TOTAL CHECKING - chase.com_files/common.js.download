/* Business Days Calculations */
var arrdts = new Array();
// Add days including weekends counted
	function daysToDate(days) {
	    var myDate = new Date();
		var newdate = myDate.getTime() + (days)*24*60*60*1000;
		var dt = new Date(newdate);
		var d =  dt.getDate();
		var y = dt.getFullYear();
		var m = dt.getMonth();		
		var months = new Array('Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov','Dec');		
		var formatDate = months[m] + " " + d + ", " + y;
	}
	
	function daysElapsed(date1,date2) {
		var difference =
			Date.UTC(date1.getFullYear(),date1.getMonth(),date1.getDate(),0,0,0)
		  - Date.UTC(date2.getFullYear(),date2.getMonth(),date2.getDate(),0,0,0);
		return difference/1000/60/60/24;
	}

// add months and days
function addMonthsDaysToDate(numMonths, days, id) {
    var startDate = new Date();
	var addYears = Math.floor(numMonths/12);
    var addMonths = numMonths - (addYears * 12);
    var newMonth = startDate.getMonth() + addMonths;
    if (startDate.getMonth() + addMonths > 11) {
      ++addYears;
      newMonth = startDate.getMonth() + addMonths - 12;
    }
	
    var newDate = new Date(startDate.getFullYear()+addYears,newMonth,startDate.getDate());
	
    // adjust to correct month
    while (newDate.getMonth() != newMonth) {
      newDate = daysToDate(-1);
    }
	var newdate = newDate.getTime() + (days)*24*60*60*1000;
	var dt = new Date(newdate);
	
	var d =  dt.getDate();
	var y = dt.getFullYear();
	var m = dt.getMonth();	
	var months = new Array('Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov','Dec');		
	var formatDate = months[m] + " " + d + ", " + y;
	
	var fd = new Date(formatDate);
	var n = daysElapsed(fd,startDate);
	addBusinessDays(n, id)
}

