function mergeTableRows(t1,t2){
	/*******************************************/
	
	//1. Extract source table
	var table = $($('table.activity-table')[0]);
	table.attr('id','activity-transactions_v2');
	//2. Create a new table
	var source = table.clone().empty();/*$('<table>');*/
	source.append(table.find('thead').clone());
	source.attr('id','activity-transactions_v1');
	var arr = [];
	//3. Iterate through table rows
	table.find('tr.solid.top.border').each(function(e,d){
		var d1,d2;
		d1 = $(d).find('.date').html();
		
		var len = arr.length;
		for(var i = 0; i < arr.length; i++){
			var cloneDiv = $(d).clone();
			var el = $(arr[i]);
			d2 = el.find('.date').html();
			if(d1==d2){
				var rows;
				cloneDiv.find('td:first-child').remove();
				/*cloneDiv.removeClass('')*/
				cloneDiv.find('td').addClass('grouped top border');
				/*cloneDiv.attr('class','no bottom border');*/
				if($(el.find('.date')[0]).attr('rowspan')){
					rows = Number($(el.find('.date')[0]).attr('rowspan'))
					var arr2 = arr.splice(i+rows);
					arr2.unshift(cloneDiv);
					arr = arr.concat(arr2);
					++rows;
					$(el.find('.date')[0]).attr('rowspan',rows)
				}
				else{
					$(el.find('.date')[0]).attr('rowspan',2);
					var arr2 = arr.splice(i+1);
					arr2.unshift(cloneDiv);
					arr = arr.concat(arr2);
				}
				break;
				
			}
		}
		if(len == arr.length){
			arr.push($(d).clone());
		}
		$(d).find('td').addClass('padding5');
	});
	
	for(var i = 0; i < arr.length; i++){
		source.append(arr[i]);
	}
	if(t1){
		var parentDiv = $(table.parent());
		parentDiv.append(source);
	}
	else{
		var parentDiv = $($('.transactionInnerContainerBoxShadow')[0]);
		parentDiv.append(source);
		parentDiv.append($('div.aligned.center.util.print-hide'))
	}
	
}


