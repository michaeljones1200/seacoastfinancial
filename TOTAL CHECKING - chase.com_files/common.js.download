			$("#transfer_funds_all_menu_item a").addClass("active");
		}
		else if (tycdLnkId == "item-1-seeRepeatingTransfers"){
			$("#main-menu").hide();
			$("#widget-grid").hide();
			$("#pay-activity").show();
			$("#paymentActivity_container").load("templates/pa_repeating_transfers.html");
			$("#bill_pay_payees_menu_item a").removeClass("active");
			$("#transfer_funds_all_menu_item a").removeClass("active");
			$("#transfer_funds_recurring_menu_item a").addClass("active");
		}
		else if (tycdLnkId == "item-2-manageExternalAccounts"){
			$("#main-menu").hide();
			$("#widget-grid").hide();
			$("#externalAccts").show();
			$("#seeEditExtAcc").removeClass("dis-none");
			$("#addExtAcc").addClass("dis-none");
		}
		else if (tycdLnkId == "item-3-addExternalAccounts"){
			$("#main-menu").hide();
			$("#widget-grid").hide();
			$("#externalAccts").show();
			$("#seeEditExtAcc").addClass("dis-none");
			$("#addExtAcc").removeClass("dis-none");
			$("#manage_external_accounts a").removeClass("active");
			$("#add_external_account a").addClass("active");
		}
		else if(tycdLnkId == "item-4-makeWireTransfers"){
			$("#main-menu").hide();
			$("#widget-grid").hide();
			$("#wireMoney").show();
		}
		
		$("#accountTransferEnter div#thingsYouCanDo").removeClass("show");
	});

});


