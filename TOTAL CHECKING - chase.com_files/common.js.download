		if($("#bottom-div-pa, #bottom-div-pb, #bottom-div-qp, #bottom-div-tm, #bottom-div-wm, #bottom-div-am, #bottom-div-ac, #bottom-div-ap, #bottom-div-gt, #bottom-div-fp, #bottom-div-pt, #pseudo-nav").is(":visible")){ 
			$("#pay-activity").css("margin-top",0);
