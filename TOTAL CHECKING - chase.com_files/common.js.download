$(function() {
$("#body").on("click", "div#outerDiv #input-transactionDueDate-input-icon, div#outerDiv #input-transferToAccounts_5012919456-datepicker-input-icon, div#outerDiv #input-transactionFromDate-datepicker-input-icon, div#outerDiv #input-transactionToDate-datepicker-input-icon, div#outerDiv #input-sendOnDate-input-icon, div#outerDiv #input-requestedBy-input-icon, div#outerDiv #input-departure-date-field-input-icon, div#outerDiv #input-return-date-field-input-icon, div#outerDiv #input-transactionInitiationDate_2-input-icon, div#outerDiv #input-transactionDueDate_2-input-icon, div#outerDiv #input-accountActivityFromDate-input-icon,  div#outerDiv #input-accountActivityToDate-input-icon,  div#outerDiv #input-transactionFromDate-input-icon,  div#outerDiv #input-transactionToDate-input-icon,  div#outerDiv #input-transactionDueDate-0-input-icon,  div#outerDiv #input-transactionDueDate-1-input-icon, div#outerDiv #input-transactionInitiationDate-input-icon_wm, div#outerDiv #input-editTransactionInitiationDateId-input-icon, div#outerDiv #input-transactionInitiationDate-input-icon_intsw, div#outerDiv #input-transactionDueDate_0-datepicker-input-icon, div#outerDiv #input-transactionInitiationDate_22-datepicker-input-icon, div#outerDiv #input-transactionDueDate_23-datepicker-input-icon, div#outerDiv #input-transactionDueDate_24-input-icon, div#outerDiv #input-transactionInitiationDate_30-datepicker-input-icon, div#outerDiv #input-transactionDueDate_303-datepicker-input-icon", function() {
		var aThis=$(this);
		
		var $calendar=$(this)
			    .parents("div#outerDiv")
			    .children("*")
			    .find("div.jpui.calendars-container")
			    .css("display","block")
			    .children("div.jpui.calendars");
				
		if($calendar.html())
		{
			if($calendar.css("display")=="block")	
				{		

					$calendar.css("display","none");
				}
			else
			{
				
				$calendar.css("display","block");
			}
		}
		if(!$calendar.html())
			{				
				mydtpicker=new dtpicker(new Date());
				var myhtml=mydtpicker.fill2();											
				$calendar.append(myhtml);
				
				
			}
		
			
				//select date from calender				
				$calendar.on("click","table td[data-af-test='day']",function(event){
					/* code for schedule wire international future dated start */
					var sw_current_date = $('.today').attr("data-date");
					var sw_selected_date = $(this).attr("data-date");
					
					var sw_a = new Date(sw_current_date);
					var sw_b = new Date(sw_selected_date);
					
					sw_a = sw_a.getTime();
					sw_b = sw_b.getTime();

						if(sw_b > sw_a){
						localStorage.setItem('futuredateclicked','yes');
						$('.futuredatetext_forsw_wm').show();
						$('.currentdatetext_forsw_wm').hide();
						$('.futuredatetext_right_for_wm').show();
						$('.futuredatetext_right_for_gt').show();
							if($('input#header-currencyIdIcons').val() != 'USD'){
								$('input#transactionAmount-text-input-field').attr('placeholder','TBD');
								$('input#transactionAmount-text-input-field').val('TBD');
								$('#wireAmount span span').text('TBD');
								$('#rate-counter').hide();
							}
							
							if($('input#header-currencyIdIcons').val() == 'USD'){
							$('.futuredatetext_right_for_wm').hide();
							}
						}
						
						else {
						 localStorage.removeItem('futuredateclicked');
						 $('.exchange_amt_ftdate').text('1,457.33 MXN (Mexican Peso)');
						 $('.futuredatetext_forsw_wm').hide();
						 $('.currentdatetext_forsw_wm').show();
						 $('.futuredatetext_right_for_wm').hide();
						 $('.futuredatetext_right_for_gt').hide();
						
						} 
					/* code for schedule wire international future dated start */
					
					mydtpicker.dayClickSingle(event,$(this));}
				);
				
				//close calender
				$("section.accountTransfer.payments, div#quickpay-content").click(function(e){
							
								
						if( ( $(e.target).prop("class") != "jpui close icon" )  && mydtpicker && e.target.type!="submit")						
						{	
											
						 if ( $(e.target).prop("id") == "input-transactionDueDate-input-icon" || $(e.target).prop("id")=="input-transactionFromDate-datepicker-input-icon" || $(e.target).prop("id")=="input-transactionToDate-datepicker-input-icon" || $(e.target).prop("id")=="input-sendOnDate-input-icon" || $(e.target).prop("id") =="input-requestedBy-input-icon" || $(e.target).prop("id")=="input-transferToAccounts_5012919456-datepicker-input-icon")
						 {}
						 else
							$calendar.css("display","none");
							
						}
					});				
				

		
				//******************** prev and next arrow click start *******************

				$(this).parents("div#outerDiv").children("*").find("div.jpui.calendars-container")
				.on("click","#next-transactionInitiationDate_0, #prev-transactionInitiationDate_0",function(event)
				{		
																			
					if( $(this).prop("id").search("next-transactionInitiationDate_0")!=-1 ){
						mydtpicker.setArrow(1);						
						event.stopImmediatePropagation();
					}
							
					if( $(this).prop("id").search("prev-transactionInitiationDate_0" )!=-1){
						mydtpicker.setArrow(-1);
						event.stopImmediatePropagation();
					}

					var mymnth=-1;
					var myyear=-1;
					if( options.locale.months.indexOf( $(this).children("span.util.accessible-text").text().split(",")[0]  )==0)		
						mymnth=11;
					else
						mymnth=options.locale.months.indexOf(  $(this).children("span.util.accessible-text").text().split(",")[0] )  -1;
						
						myyear=$(this).children("span.util.accessible-text").text().split(",")[1] ;
						
					


												
					if($(this).prop("id")=="next-transactionInitiationDate_0")
					{
						mymnth=mymnth+1;
					}
					else if ($(this).prop("id")=="prev-transactionInitiationDate_0")
					{
						mymnth=mymnth-1;
						if(mymnth==-1){mymnth=11;myyear=myyear-1;}
					}								
																
					var myhtml=mydtpicker.fill2(mymnth,myyear);											
					$calendar.find("*").filter("div.jpui.datepicker.calendars").remove();
					$calendar.append(myhtml);
							
				}
				);
			//******************** prev and next arrow click end*******************


}
);

});

